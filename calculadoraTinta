y = int(input('Qual é o rendimento da sua lata de tinta?:'))
altura = int(input('Qual é a altura da sua parede?:'))
largura = int(input('Qual é a largura da sua parede?:'))

def pintar():
    area = altura * largura
    total = area / y
    print(f'Você precisará de {total} latas de tinta para pintar sua parede')

pintar()
